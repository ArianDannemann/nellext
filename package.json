{
    "name": "nellext",
    "displayName": "nellext",
    "description": "language support for nell",
    "publisher": "ArianDannemann",
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/ArianDannemann/nellext"
    },
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "nell",
            "aliases": ["nell", "nell"],
            "extensions": [".nell"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nell",
            "scopeName": "source.nell",
            "path": "./syntaxes/nell.tmLanguage.json"
        }]
    }
}
